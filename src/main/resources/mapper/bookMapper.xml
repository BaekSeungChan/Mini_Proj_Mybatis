<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.minproj2_mybatis.book.mapper.BookCartMapper">
    <insert id="save" parameterType="book">
        INSERT INTO book (username, title, link, author, discount, publisher, isbn, quantity) VALUES (#{username}, #{title}, #{link}, #{author}, #{discount}, #{publisher}, #{isbn}, #{quantity})
    </insert>

